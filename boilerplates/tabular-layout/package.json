{
    "name": "tabular-layout",
    "description": "Please enter a description for your extension",
    "icon": "extension",
    "version": "1.0.0",
    "keywords": [
        "directus",
        "directus-extension",
        "directus-extension-layout",
        "directus-extension-tabular-layout",
        "directus-extension-table-layout",
        "tabular-layout",
        "table-layout"
    ],
    "type": "module",
    "files": [
        "dist"
    ],
    "directus:extension": {
        "type": "layout",
        "path": "dist/index.js",
        "source": "src/index.ts",
        "host": "^10.10.0 || ^11.0.0"
    },
    "scripts": {
        "build": "directus-extension build",
        "dev": "directus-extension build -w --no-minify",
        "link": "directus-extension link"
    },
    "devDependencies": {
        "@directus/extensions-sdk": "12.1.1",
        "typescript": "^5.6.3",
        "vue": "^3.5.12"
    }
}
